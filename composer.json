{
    "name": "tarik02/laravel-telegram",
    "description": "Telegram framework for Laravel.",
    "keywords": [
        "laravel",
        "telegram"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "tarik02/telegram-types": "^0.1.6",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/bus": "^7.0|^8.0",
        "illuminate/queue": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarik02\\LaravelTelegram\\Providers\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "Tarik02\\LaravelTelegram\\Facades\\Telegram"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Tarik02\\LaravelTelegram\\": "src/"
        }
    }
}
